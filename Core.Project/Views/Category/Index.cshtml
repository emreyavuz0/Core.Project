@using Core.EntityLayer.Concrete
@model List<Category>
@{
    ViewData["Title"] = "Index";
}

<table>
    <tr>
        <th>ID</th>
        <th>Kategori Adı</th>
        <th>Durumu</th>
        <th>Detaylar</th>
        <th>Sil</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.CategoryId</td>
            <td>@item.CategoryName</td>
            <td>@item.CategoryStatus</td>
            <td>@item.CategoryDescription</td>
        </tr>
    }

</table>

